@model string
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="description" content="@Localizer["metaDescription"].Value" />
<meta name="twitter:card" content="@Localizer["metaTwitterCard"].Value" />
<meta property="og:title" content="@Model" />
<meta property="og:type" content="article" />
<meta property="og:url" content="https://the-example-app-csharp.contentful.com/" />
<meta property="og:image" content="https://the-example-app-csharp.contentful.com/og-image.jpg" />
<meta property="og:image:type" content="image/jpeg" />
<meta property="og:image:width" content="1200" />
<meta property="og:image:height" content="1200" />
<meta property="og:image:alt" content="@Localizer["metaImageAlt"].Value" />
<meta property="og:description" content="@Localizer["metaImageDescription"].Value" />
<link rel="apple-touch-icon" sizes="120x120" href="/apple-touch-icon.png" />
<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
<link rel="manifest" href="/manifest.json" />
<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#4a90e2" />
<meta name="theme-color" content="#ffffff" />

<title>@Model</title>